{
    "cleanUrls": true,
    "rewrites": [
      {
        "source": "/(manifest\\.json|manifest\\.webmanifest|favicon\\.ico|favicon\\.png|apple-touch-icon\\.png|icons/.*|assets/.*|_expo/.*)",
        "destination": "/$1"
      },
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ],
    "headers": [
      {
        "source": "/(manifest\\.json|manifest\\.webmanifest)",
        "headers": [{ "key": "Cache-Control", "value": "no-cache" }]
      }
    ]
  }
  